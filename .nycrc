{
    "reporter": [
        "text",
        "html"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 60,
    "statements": 60,
    "functions": 60,
    "branches": 60,
    "watermarks": {
        "lines": [60, 80],
        "functions": [60, 80],
        "branches": [60, 80],
        "statements": [60, 80]
    },
    "report-dir": "./dist/test-coverage",
    "exclude": [
        "tests"
    ]
}